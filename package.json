{
  "name": "vue-spa-template",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && rm -rf dist && vite build",
    "serve": "vite preview --port 8080",
    "check": "vue-tsc --noEmit && eslint 'src/**/*.{ts,tsx,vue}' && prettier -cu --ignore-path .gitignore .",
    "lint": "eslint 'src/**/*.{ts,tsx,vue}'",
    "fix": "eslint --fix 'src/**/*.{ts,tsx,vue}'",
    "format": "prettier -cwu --ignore-path .gitignore ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/curegit/vue-spa-template.git"
  },
  "author": "curegit (https://github.com/curegit)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/curegit/vue-spa-template/issues"
  },
  "homepage": "https://github.com/curegit/vue-spa-template",
  "dependencies": {
    "vue": "^3.2.0",
    "vue-router": "^4.1.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "eslint": "^8.30.0",
    "eslint-plugin-vue": "^9.8.0",
    "prettier": "^2.8.0",
    "typescript": "^4.9.0",
    "vite": "^4.0.0",
    "vue-tsc": "^1.0.0"
  }
}
